testCase=int(input())
while(testCase):
    string=input().lower()
    half=len(string)//2
    if half % 2==0:
        if (sorted(string[:half])==sorted(string[half:])):
            print("Yes")
        else: print("No")
    else:
        if(sorted(string[:half])==sorted(string[half+1:])):
            print("Yes")
        else: print("No")
    testCase-=1
